{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node16",  
    "module": "Node16",             
    "esModuleInterop": true,       
    "skipLibCheck": true,          
    "types": ["react", "react-native", "@react-navigation/native"],  
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
